{
  "name": "nmdwall-server",
  "version": "1.0.0",
  "description": "an interactive wall",
  "main": "server.js",
  "scripts": {
    "pretest": "jshint **.js",
    "test": "echo test completed",
    "start": "node server.js"
  },
  "author": "Sarah Federman",
  "license": "ISC",
  "dependencies": {
    "body-parser": "~1.0.1",
    "express": "^4.0.0",
    "express-session": "^1.12.1",
    "mongoose": "~3.6.13",
    "multer": "^1.1.0",
    "node-env-file": "^0.1.7"
  },
  "devDependencies": {
    "node-env-file": "^0.1.7"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "sub": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "boss": true,
    "eqnull": true,
    "strict": true,
    "immed": true,
    "expr": true,
    "latedef": "nofunc",
    "quotmark": "single",
    "indent": 2,
    "node": true
  }
}
